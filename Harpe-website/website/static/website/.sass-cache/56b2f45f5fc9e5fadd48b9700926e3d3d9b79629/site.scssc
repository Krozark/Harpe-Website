o:Sass::Tree::RootNode
:@template"l9@import "css3.scss";

$width: 980px;

$basecolor : #4a4a4a;
$menucolor: $basecolor;
$linkhover : #0ad6e7;
$linkcolor : #bbb3ac;
$bordercolor : $menucolor + #222;


.right { float: right;}
.left { float: left;}

.link {
    font-weight: normal;
    text-decoration: none;
    color: $linkcolor;	
    &:hover {
        color: $linkhover;
    }
}

.divider { 
    margin-bottom: 40px; 
    padding-bottom: 20px; 
    border-bottom: 1px solid #ccc;
}

.bordered {
    border : $bordercolor 1px solid;
    @include border-radius(5px);
}

.col { 
    float: left; 
    margin: 0 20px 20px 0; 
}

.one_fourth {
    width : $width /4 - 20;
}

a {
    @extend .link;
    &:hover {
        text-decoration: underline;
    }
}



p { 
    margin: 0 0 10px 0;
    padding: 0;
}
img {
    border: none;
}
blockquote {
    font-style: italic;
    margin: 0 0 0 10px;
}

h1, h2, h3, h4, h5, h6 { 
    color: $basecolor;
    font-weight: normal;
}
h1 { 
    font-size: 48px;
    margin: 0 0 30px;
    padding: 5px 0;
}
h2 { 
    font-size: 30px; 
    margin: 0 0 25px; 
    padding: 5px 0;
}
h3 { 
    font-size: 24px; 
    margin: 0 0 20px; 
    padding: 0;
}
h4 { 
    font-size: 18px; 
    margin: 0 0 15px; 
    padding: 0;
}
h5 { 
    font-size: 16px; 
    margin: 0 0 10px; 
    padding: 0;
}
h6 { 
    font-size: 12px; 
    margin: 0 0 5px; 
    padding: 0;
}

.pagination {
    @extend .clearfix;
    margin: 0 0 20px; 
    padding: 0;

    a { 
        float: left; 
        display: block; 
        color: #333; 
        text-decoration: none; 
        margin-right: 5px; 
        padding: 5px 10px; 
        background: #fff; 
        border: 1px solid $bordercolor;

        &:hover { 
            background: #ccc; 
            border: 1px solid #fff;
            color: #fff;  
        }
    }
}

.btn { 
    padding: 5px 14px; 
    font-weight: bold;  
    color: #222; 
    background: #fcfcfc; 
    border: 1px solid #dedede; 
    font-family: Tahoma, Geneva, sans-serif;
    font-size: 13px;
    margin: 10px 0px; 
    width : 100px;

    &:hover { 
        background: #ccc; 
        border: 1px solid #fff;
        color: #fff;  
    }
}	

body {
     background-image: url(../img/body_head.png);
     background-repeat: repeat-x;
     background-position: top;

     margin: 0;
     padding: 0;
     color: #666;
     font-family: Tahoma, Geneva, sans-serif;
     font-size: 14px;
     line-height: 1.4em; 
}

.wrapper {
    width : $width;
    margin: 0 auto;
}

.header {
    .wrapper {
        background: url(../img/header.jpg) top center no-repeat;
        padding: 40px 0 0;
        height: 80px;
    }

    #title {
        @extend .left;
        a {
            display: block; 
            width: 186px; 
            height: 10px; 
            padding: 40px 10px 0 0; 
            color: #fff; 
            font-size: 14px; 
            text-align: right; 
            background: url(../img/logo.png) no-repeat left center; 
        }

    }

    .menu {
        @extend .right;
        @extend .clearfix;
        @extend .bordered;

        width : $width - 300;
        height : 46px;
        //background: url(../img/menu.png) no-repeat; 
        //background: $menucolor;
        @include background-gradient($menucolor,$menucolor - #222);

        .nav {
           list-style-type: none;

           .item {
               &:hover {
                   background-color : $bordercolor;
               }
               .link {
                    color: #bbb3ac;	
                    &.active, &:hover {
                        color: $linkhover;
                    }
               }
           }
        }
        
        .nav-0 {
           padding: 0 0 0 30px;
            .item {
                &:hover {
                    & > ul {
                        float : right;
                        display:inline;
                    }
                }
            }
            .item-1 {
                float : left;
                //@extend .left;
                margin-right : 5px;
                

                &:last-child {
                    margin-right: 0px;
                }

                .link {
                    display: block;
                    //height: 42px;
                    padding: 0 30px 0 20px;
                    line-height: 42px;
                    font-size: 14px;
                    //text-align: center;
                    text-decoration: none;
                    font-weight: 400;
                    outline: none;

                }

                .nav {
                    @extend .bordered;
                    display : none;
                    position : absolute;
                    @include background-gradient($menucolor,$menucolor - #222);
                    .item {
                        border-bottom : $bordercolor 1px solid;
                    }
                }
                &:hover {
                    & > ul {
                        display:block;
                    }
                }
            }
        }
    }
}

ul.breadcrumb {
    @extend .right;
    padding-bottom : 10px;
    list-style: none; 
    li {
        float : left;
        margin-right : 3px;
        span.self {
            color : $linkhover * 0.9;
        }

        a {
        }

        span.divider {
        }

    }
}

#main {
    @extend .clearfix;
    width : 100%;
    padding: 40px 0;

    a { 
        color: $linkcolor - #333;
        font-weight: normal; 

        &:hover { 
            color: $linkhover; 
        }
    }



    .wrapper {
        @extend .clearfix;
        background: url(../img/vdivider.png) no-repeat scroll $width/5.0 - 20 0px;  

        #sidebar {
            @extend .left;
            width: $width /5.0 - 20;

            ul {
                margin: 10px 0 10px 0; 
                padding: 0; 
                list-style: none; 

                margin-bottom: 40px; 
                padding-bottom: 20px; 
                //border-bottom: 1px solid #ccc;
                background: url(../img/divider.png) bottom left no-repeat ;  

                &:last-child {
                    //border : none;
                    background-image : none;
                }

                li { 
                    margin: 0 0 2px 0; 
                    padding: 0 5px 0 20px; 
                    background: url(../img/list.png) no-repeat scroll 0 7px;  
                }
            }
        }

        #content {
            @extend .right;
            width : $width/5.0 * 4; 

            form {
                @extend .clearfix;
                padding: 0; 
                width: 410px; 
                margin-bottom: 40px; 

                label { 
                    display: block; 
                    width: 100px; 
                    margin-right: 12px; 
                    font-size: 13px; 
                }

                ul.errorlist {
                    @extend .right;
                    margin-right: 12px; 
                    margin-top: -18px; 
                    list-style: none; 

                    li {
                        color: #F00;
                    }
                }

                input { 
                    width: 270px; 
                    padding: 5px; 
                    color: #222;  
                    background: #fcfcfc; 
                    border: 1px solid #dedede; 
                    font-size: 12px;
                    font-family: Tahoma, Geneva, sans-serif;
                    margin-top: 5px; 
                }

                textarea { 
                    width: 398px; 
                    height: 200px; 
                    padding: 5px; 
                    color: #222; 
                    background: #fcfcfc; 
                    border: 1px solid #dedede; 
                    font-size: 12px;
                    font-family: Tahoma, Geneva, sans-serif;
                    margin-top: 5px; 
                }


            }

            .post {
                padding : 10px;
                margin-bottom : 10px;

                //@include box-shadow(2px,2px,3px);
                //background-color : #EEE;

                //background: url(../img/divider_2.png) bottom center no-repeat ;  

                @include box-shadow(-3px,-3px,8px);
                @include border-radius();
                @include background-gradient(#EEE,#FFF);

                &:nth-child(2n+2) {  
                    @include background-gradient(#EEE - #111 ,#FFF - #111);
                }

                .title {
                    @extend .clearfix;
                    h2 {
                        @extend .left;
                    }
                    span { 
                        @extend .right;
                        display: block; 
                        margin-right: 15px; 
                        padding: 5px 0 5px 26px; 

                        &.date { 
                            background: url(../img/calendar.png) no-repeat; 
                        }
                    }
                }

                .entry {
                    p {
                        text-indent : 15px;
                        margin-bottom : 5px;
                    }

                    table {
                        font-size: 0.9em;
                        width : 100%;
                        margin-bottom : 10px;

                        tr {
                            td {
                                border: 1px solid #CCC;
                                padding : 3px;
                            }
                        }
                    }
                }

                .meta {
                    @extend .clearfix;

                    .byline {
                        @extend .left;
                    }
                    .comments {
                        @extend .right;

                        .more {
                            color : $linkhover *0.9;
                        }
                    }
                }
            }

        }
    }
}

#footer {
    @extend .clearfix;
    width: 100%;
    background: #303030 url(../img/footer.png) repeat-x top;

    .wrapper {
        @extend .clearfix;
        //width : $width;
        padding: 80px 10px 40px;
        color: #999;

        font-size: 0.8em;

        a { 
            color: #ccc; 
        }

        ul { 
            margin: 0; 
            padding: 0; 
            list-style: none; 

            li { 
                margin: 0 0 5px; 
                padding: 0 0 5px; 
                border-bottom: 1px dotted $bordercolor; 
            }
        }



        h4 { 
            color: #fff; 
            font-size: 14px; 
            font-weight: 700; 
            margin-bottom: 20px; 
        }
        
        .social_button {
            @extend .clearfix;
            margin-bottom: 40px; 

            .facebook {
                @extend .left;
                margin-right: 5px; 
                width : 24px;
                height : 24px;
                background: url("../img/facebook.png") no-repeat;
            }

            .flickr {
                @extend .left;
                margin-right: 5px; 
                width : 24px;
                height : 24px;
                background: url("../img/flickr.png") no-repeat;
            }
            .twitter {
                @extend .left;
                margin-right: 5px; 
                width : 24px;
                height : 24px;
                background: url("../img/twitter.png") no-repeat;
            }
            .youtube { 
                @extend .left;
                margin-right: 5px; 
                width : 24px;
                height : 24px;
                background: url("../img/youtube.png") no-repeat;
            }
            .feed {
                @extend .left;
                margin-right: 5px; 
                width : 24px;
                height : 24px;
                background: url("../img/feed.png") no-repeat;
            }
        }
        
    }
}

div.icon {
    padding: 6px 6px 10px 22px;
    background: url(../img/docicons/unknown.png) no-repeat 0 7px transparent;

    span { //filename
    }
}
